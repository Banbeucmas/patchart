#!/bin/sh

initializeANSI(){
  esc=""
  reset="${esc}[0m"

  border="${reset}${esc}[38;5;232m"

  red="${esc}[38;5;160m";    redd="${esc}[38;5;124m"
  greyd="${reset}${esc}[38;5;235m"; grey="${reset}${esc}[38;5;250m"
  whited="${reset}${esc}[38;5;251m"; white="${reset}${esc}[38;5;255m"
  yellowd="${reset}${esc}[38;5;208m"; yellow="${reset}${esc}[38;5;214m"; yellowb="${reset}${esc}[38;5;220m"
  haird="${reset}${esc}[38;5;172m"; hair="${reset}${esc}[38;5;214m"; hairb="${reset}${esc}[38;5;226m"; hairw="${reset}${esc}[38;5;228m"
  clothd="${reset}${esc}[38;5;55m"; cloth="${reset}${esc}[38;5;57m"
  snaked="${reset}${esc}[38;5;29m"; snake="${reset}${esc}[38;5;35m"
  blued="${reset}${esc}[38;5;26m"; blue="${reset}${esc}[38;5;33m"; bluew="${reset}${esc}[38;5;69m"
  brownd="${esc}[38;5;94m"; brown="${esc}[38;5;100m"; brownb="${esc}[38;5;136m";
  


  humand="${esc}[38;5;216m"; human="${esc}[38;5;223m"; humanb="${esc}[38;5;229m"
  eyed="${reset}${esc}[38;5;235m"; eye="${reset}${esc}[38;5;240m"; eyeb="${reset}${esc}[38;5;247m"; eyew="${reset}${esc}[38;5;255m"
  boldon="${esc}[1m";    boldoff="${esc}[22m"
  italicson="${esc}[3m"; italicsoff="${esc}[23m"
  ulon="${esc}[4m";      uloff="${esc}[24m"
  invon="${esc}[7m";     invoff="${esc}[27m"
 
}

initializeANSI

